      <xs:element minOccurs="0" name="currentNaiList" type="tns:NaiListType">
        <xs:annotation>
          <xs:documentation>List of current NAI records</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="futureNaiList" type="tns:FutureNaiListType">
        <xs:annotation>
          <xs:documentation>List of future NAI records</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="expiredNaiList" type="tns:NaiListType">
        <xs:annotation>
          <xs:documentation>List of expired NAI records.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="ipDetailInfoList" type="tns:IpInfoDetailsType">
        <xs:annotation>
          <xs:documentation>Lists current and expired IP records.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="areMorePricePlans" type="tns:MoreRecordsAvailableFlagType">
        <xs:annotation>
          <xs:documentation>TRUE if there are more price plans on the subscription than could be returned in pricePlanList.  </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="pricePlanList" type="tns:PricePlanListType">
        <xs:annotation>
          <xs:documentation>List of price plan records sorted in descending effective date and time, and ascending service name order.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="subscriptionEffectiveDate" type="tns:NetworkAccessActivationFromDateType">
        <xs:annotation>
          <xs:documentation>Earliest effective date of any service</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="subscriptionExpireDate" type="tns:ProductDeploymentEffectiveThruDateType">
        <xs:annotation>
          <xs:documentation>The date the last service was expired.  If no services have expired, or none have existed, set to null.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="subscriptionId" type="tns:SubscriptionIdentifierType">
